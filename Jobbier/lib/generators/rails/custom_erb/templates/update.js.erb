$('#modal_one').modal('toggle');
<%% case params[:source] %>
<%% when '<%= singular_table_name %>_index' %>
$('#<%= singular_table_name %>_<%%= @<%= singular_table_name %>.id %>').replaceWith('<%%= j render partial: '<%= singular_table_name %>', locals: {<%= singular_table_name %>: @<%= singular_table_name %>, visible: false} %>');
$('#<%= singular_table_name %>_<%%= @<%= singular_table_name %>.id %>').toggle("highlight", {color: '#FFFFBB'}, 1000);
<%% end %>

$(document).trigger('flash:send', <%%= raw flash.to_hash.to_json %>);
<%% flash.clear %>
App.init();




